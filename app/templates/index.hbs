<ul>
  {{#each @model as |match|}}
    <li>
      <LinkTo @route="match" @model={{match.id}}>
        {{match.id}}
        [
        {{#each match.participants as |participant|}}
          {{participant.user.name}}
        {{/each}}
        ]
      </LinkTo>
    </li>
  {{/each}}
</ul>

<p>
  <LinkTo @route="create">New Game</LinkTo>
</p>
